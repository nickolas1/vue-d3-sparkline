{
  "name": "vue-d3-sparkline",
  "version": "0.1.5",
  "private": false,
  "main": "dist/vue-d3-sparkline.common",
  "homepage": "https://github.com/nickolas1/vue-sparkline",
  "files": [
    "dist/",
    "src/components/",
    "src/util/",
    "src/stories/"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target lib src/components/SparkLine.vue",
    "test": "vue-cli-service test",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-scale": "^2.0.0",
    "d3-shape": "^1.2.0",
    "lodash.debounce": "^4.0.8",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.4.1",
    "@storybook/addon-notes": "^3.4.1",
    "@storybook/vue": "^3.4.1",
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-prettier": "^3.0.0-beta.6",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
